#PBS -l nodes=1:ppn=8
#PBS -l walltime=01:00:00
#PBS -N matlab_test
#PBS -S /bin/bash

source /etc/profile.d/modules.sh
module load matlab

cd $PBS_O_WORKDIR

echo starting Matlab jobs at `date`

# Call your matlab code in file hello.m
matlab -nojvm -nodisplay -r "hello.m"

echo finished Matlab jobs at `date`


